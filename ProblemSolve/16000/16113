#include<iostream>
#include<string>
#include<vector>
using namespace std;

string input;

string number[10] = { { '#','#','#',
			'#','.','#',
			'#','.','#',
			'#','.','#',
			'#','#','#' } ,
	{'#','#','#',
'#','#','#',
'#','#','#',
'#','#','#',
'#','#','#'},

	{ '#','#','#',
			'.','.','#',
			'#','#','#',
			'#','.','.',
			'#','#','#' },
	{ '#','#','#',
			'.','.','#',
			'#','#','#',
			'.','.','#',
			'#','#','#' },
	{ '#','.','#',
			'#','.','#',
			'#','#','#',
			'.','.','#',
			'.','.','#' },
	{ '#','#','#',
			'#','.','.',
			'#','#','#',
			'.','.','#',
			'#','#','#' },

	{ '#','#','#',
			'#','.','.',
			'#','#','#',
			'#','.','#',
			'#','#','#' },
	 { '#','#','#',
			'.','.','#',
			'.','.','#',
			'.','.','#',
			'.','.','#' },
	{ '#','#','#',
			'#','.','#',
			'#','#','#',
			'#','.','#',
			'#','#','#' },
	 { '#','#','#',
			'#','.','#',
			'#','#','#',
			'.','.','#',
			'#','#','#' }
};
int main() {
	vector<int> ans;
	int N; cin >> N;
	cin >> input;

	int cnt = N / 5;
	for (int i = 0; i < cnt; i++) {
		while (input[i] == '.') {
			i++;
		}
		if (i >= cnt) break;

		if (i + 1 >= cnt) {
			ans.push_back(1);
			i += 1;
		}
		else if ((input[i+1] == '.' && input[i+1+cnt] == '.')
			&&input[i+1+2*cnt] == '.') 
		{
			ans.push_back(1);
			i += 1;
		}
		else {
			string tmp;
			for (int row = 0; row < 5; row++) {
				for (int col = i; col < i + 3; col++) {
					tmp.push_back(input[col + (row * cnt)]);
				}
			}
			for (int t = 0; t <= 9; t++) {
				if (tmp == number[t]) {
					ans.push_back(t);
					break;
				}
			}
			i += 2;
		}
	}
	for (int cur : ans) {
		cout << cur;
	}
}
